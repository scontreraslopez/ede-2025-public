@startuml class-library
class Usuario {
  - id: int
  - nombre: String
  - email: String
  + prestar()
  + devolver()
}

class Prestamo {
  - fechaInicio: Date
  - fechaFin: Date
  - devuelto: boolean
  + devolver()
  + estaVencido(): boolean
}

class Libro {
  - isbn: String
  - titulo: String
  - autor: String
  + getInfo(): String
}

Usuario "1" --> "0..*" Prestamo
Prestamo "1" --> "1" Libro
@enduml